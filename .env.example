# Example .env file for Cosmic Catalyst
# Copy this file to .env and fill in your actual values
# IMPORTANT: Never commit your actual .env file to version control!

# Supabase credentials
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# Game configuration
VITE_GAME_TITLE=Cosmic Catalyst
VITE_DEFAULT_DIFFICULTY=medium
VITE_ENABLE_ANALYTICS=true

# Social sharing
VITE_ENABLE_X_SHARING=true
VITE_SHARE_HASHTAGS=#CosmicCatalyst #SpaceShooter 